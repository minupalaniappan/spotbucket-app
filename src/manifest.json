{
  "manifest_version": 2,
  "version": "1.0",
  "name": "hoopcam",
  "description": "",
  "content_scripts": [
    {
      "run_at": "document_idle",
      "js": ["inject.bundle.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "web_accessible_resources": ["index.bundle.js"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
